{
    "clearMocks": true,
    "coverageDirectory": "./docs/coverage",
    "collectCoverage": true,
    "maxWorkers": 1,
    "moduleFileExtensions": [
        "js",
        "json",
        "jsx",
        "ts",
        "tsx",
        "node"
    ],
    "testEnvironment": "node",
    "preset": "ts-jest",
    "testMatch": [
        "**/test/*.spec.ts"
    ]
}